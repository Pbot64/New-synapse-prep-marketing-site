{
  "version": 2,
  "alias": ["synapseprep.net"],

  "builds": [
    {
      "src": "next.config.js",
      "use": "@now/next"
    },
    { "src": "/lamdas/*.js", "use": "@now/node" }
  ],

  "routes": [
    { "src": "/checkout/(?<name>[^/]+)$", "dest": "/checkout?name=$name" },

    { "src": "/api/test", "dest": "/lamdas/test.js" },
    { "src": "/api/contact", "dest": "/lamdas/contact.js" }
  ]
}
